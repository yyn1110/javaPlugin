<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.CouponUserMiddleDao">

	<!--属性-->
	<parameterMap id="couponUserMiddleParameterMap" type="com.yao.yz.kubauser.persistence.model.CouponUserMiddle">
		<parameter property="batchcode"/>
		<parameter property="code"/>
		<parameter property="username"/>
		<parameter property="senddate"/>
		<parameter property="status"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="couponUserMiddleResultMap" type="com.yao.yz.kubauser.persistence.model.CouponUserMiddle">
		<result property="batchcode" column="batchcode"/>
		<result property="code" column="code"/>
		<result property="username" column="username"/>
		<result property="senddate" column="senddate"/>
		<result property="status" column="status"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="couponUserMiddleParameterMap">
		insert into coupon_user_middle(`batchcode`,`code`,`username`,`senddate`,`status`) values(?,?,?,?,?)
		<!--没有主键-->
	</insert>

	<!--更新-->
	<update id="update" parameterMap="couponUserMiddleParameterMap">
		update coupon_user_middle set
		<trim suffixOverrides=",">
			<if test="batchcode != null">
				`batchcode`=#{batchcode},
			</if>
			<if test="code != null">
				`code`=#{code},
			</if>
			<if test="username != null">
				`username`=#{username},
			</if>
			<if test="senddate != null">
				`senddate`=#{senddate},
			</if>
			<if test="status != null">
				`status`=#{status},
			</if>
		</trim>
		<!--没有主键-->
	</update>

	<!--删除-->
	<!--没有主键-->

	<!--查询-->
	<!--没有主键-->

</mapper>
