<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.EcuserChangeprivilegeDao">

	<!--属性-->
	<parameterMap id="ecuserChangeprivilegeParameterMap" type="com.yao.yz.kubauser.persistence.model.EcuserChangeprivilege">
		<parameter property="id"/>
		<parameter property="userName"/>
		<parameter property="price"/>
		<parameter property="count"/>
		<parameter property="successCount"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="ecuserChangeprivilegeResultMap" type="com.yao.yz.kubauser.persistence.model.EcuserChangeprivilege">
		<result property="id" column="Id"/>
		<result property="userName" column="UserName"/>
		<result property="price" column="Price"/>
		<result property="count" column="Count"/>
		<result property="successCount" column="SuccessCount"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="ecuserChangeprivilegeParameterMap" keyProperty="id" useGeneratedKeys="true">
		insert into ecuser_changeprivilege(`Id`,`UserName`,`Price`,`Count`,`SuccessCount`) values(?,?,?,?,?)
	</insert>

	<!--更新-->
	<update id="update" parameterMap="ecuserChangeprivilegeParameterMap">
		update ecuser_changeprivilege set
		<trim suffixOverrides=",">
			<if test="userName != null">
				`UserName`=#{userName},
			</if>
			<if test="price != null">
				`Price`=#{price},
			</if>
			<if test="count != null">
				`Count`=#{count},
			</if>
			<if test="successCount != null">
				`SuccessCount`=#{successCount},
			</if>
		</trim>
		where `Id`=#{id}
	</update>

	<!--删除-->
	<delete id="delete" parameterType="java.lang.Integer">
		delete from ecuser_changeprivilege where `Id`=#{id}
	</delete>

	<!--查询-->
	<select id="getEcuserChangeprivilegeByKey" resultMap="ecuserChangeprivilegeResultMap" parameterType="java.lang.Integer">
		select `Id`,`UserName`,`Price`,`Count`,`SuccessCount` from ecuser_changeprivilege where `Id`=#{id}
	</select>

</mapper>
