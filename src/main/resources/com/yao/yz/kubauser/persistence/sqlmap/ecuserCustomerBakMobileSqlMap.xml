		<trim suffixOverrides=",">
			<if test="cellphone != null">
				`Cellphone`=#{cellphone},
			</if>
		</trim>
		<!--没有主键-->
	</update>

	<!--删除-->
	<!--没有主键-->

	<!--查询-->
	<!--没有主键-->

</mapper>
