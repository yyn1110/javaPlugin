	<insert id="insert" parameterMap="ecuserCustomerBlacklistParameterMap" keyProperty="autoId" useGeneratedKeys="true">
		insert into ecuser_customer_blacklist(`AutoId`,`UserId`,`UserName`,`Mobile`,`Tel`,`UserIp`,`Creator`,`CreateTime`,`IsDeleted`) values(?,?,?,?,?,?,?,?,?)
