<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.EcuserSensitivewordsDao">

	<!--属性-->
	<parameterMap id="ecuserSensitivewordsParameterMap" type="com.yao.yz.kubauser.persistence.model.EcuserSensitivewords">
		<parameter property="id"/>
		<parameter property="wordContent"/>
		<parameter property="addTime"/>
		<parameter property="addUser"/>
		<parameter property="status"/>
		<parameter property="type"/>
		<parameter property="updateUser"/>
		<parameter property="updateTime"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="ecuserSensitivewordsResultMap" type="com.yao.yz.kubauser.persistence.model.EcuserSensitivewords">
		<result property="id" column="Id"/>
		<result property="wordContent" column="WordContent"/>
		<result property="addTime" column="AddTime"/>
		<result property="addUser" column="AddUser"/>
		<result property="status" column="Status"/>
		<result property="type" column="Type"/>
		<result property="updateUser" column="updateUser"/>
		<result property="updateTime" column="updateTime"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="ecuserSensitivewordsParameterMap" keyProperty="id" useGeneratedKeys="true">
		insert into ecuser_sensitivewords(`Id`,`WordContent`,`AddTime`,`AddUser`,`Status`,`Type`,`updateUser`,`updateTime`) values(?,?,?,?,?,?,?,?)
	</insert>

	<!--更新-->
	<update id="update" parameterMap="ecuserSensitivewordsParameterMap">
		update ecuser_sensitivewords set
		<trim suffixOverrides=",">
			<if test="wordContent != null">
				`WordContent`=#{wordContent},
			</if>
			<if test="addTime != null">
				`AddTime`=#{addTime},
			</if>
			<if test="addUser != null">
				`AddUser`=#{addUser},
			</if>
			<if test="status != null">
				`Status`=#{status},
			</if>
			<if test="type != null">
				`Type`=#{type},
			</if>
			<if test="updateUser != null">
				`updateUser`=#{updateUser},
			</if>
			<if test="updateTime != null">
				`updateTime`=#{updateTime},
			</if>
		</trim>
		where `Id`=#{id}
	</update>

	<!--删除-->
	<delete id="delete" parameterType="java.lang.Integer">
		delete from ecuser_sensitivewords where `Id`=#{id}
	</delete>

	<!--查询-->
	<select id="getEcuserSensitivewordsByKey" resultMap="ecuserSensitivewordsResultMap" parameterType="java.lang.Integer">
		select `Id`,`WordContent`,`AddTime`,`AddUser`,`Status`,`Type`,`updateUser`,`updateTime` from ecuser_sensitivewords where `Id`=#{id}
	</select>

</mapper>
