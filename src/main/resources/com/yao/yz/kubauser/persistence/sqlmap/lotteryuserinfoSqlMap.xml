<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.LotteryuserinfoDao">

	<!--属性-->
	<parameterMap id="lotteryuserinfoParameterMap" type="com.yao.yz.kubauser.persistence.model.Lotteryuserinfo">
		<parameter property="userName"/>
		<parameter property="tel"/>
		<parameter property="userId"/>
		<parameter property="id"/>
		<parameter property="address"/>
		<parameter property="remark"/>
		<parameter property="actType"/>
		<parameter property="ip"/>
		<parameter property="goodsName"/>
		<parameter property="goodsId"/>
		<parameter property="realName"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="lotteryuserinfoResultMap" type="com.yao.yz.kubauser.persistence.model.Lotteryuserinfo">
		<result property="userName" column="UserName"/>
		<result property="tel" column="Tel"/>
		<result property="userId" column="UserId"/>
		<result property="id" column="Id"/>
		<result property="address" column="Address"/>
		<result property="remark" column="Remark"/>
		<result property="actType" column="ActType"/>
		<result property="ip" column="Ip"/>
		<result property="goodsName" column="GoodsName"/>
		<result property="goodsId" column="GoodsId"/>
		<result property="realName" column="RealName"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="lotteryuserinfoParameterMap" keyProperty="id" useGeneratedKeys="true">
		insert into lotteryuserinfo(`UserName`,`Tel`,`UserId`,`Id`,`Address`,`Remark`,`ActType`,`Ip`,`GoodsName`,`GoodsId`,`RealName`) values(?,?,?,?,?,?,?,?,?,?,?)
	</insert>

	<!--更新-->
	<update id="update" parameterMap="lotteryuserinfoParameterMap">
		update lotteryuserinfo set
		<trim suffixOverrides=",">
			<if test="userName != null">
				`UserName`=#{userName},
			</if>
			<if test="tel != null">
				`Tel`=#{tel},
			</if>
			<if test="userId != null">
				`UserId`=#{userId},
			</if>
			<if test="address != null">
				`Address`=#{address},
			</if>
			<if test="remark != null">
				`Remark`=#{remark},
			</if>
			<if test="actType != null">
				`ActType`=#{actType},
			</if>
			<if test="ip != null">
				`Ip`=#{ip},
			</if>
			<if test="goodsName != null">
				`GoodsName`=#{goodsName},
			</if>
			<if test="goodsId != null">
				`GoodsId`=#{goodsId},
			</if>
			<if test="realName != null">
				`RealName`=#{realName},
			</if>
		</trim>
		where `Id`=#{id}
	</update>

	<!--删除-->
	<delete id="delete" parameterType="java.lang.Integer">
		delete from lotteryuserinfo where `Id`=#{id}
	</delete>

	<!--查询-->
	<select id="getLotteryuserinfoByKey" resultMap="lotteryuserinfoResultMap" parameterType="java.lang.Integer">
		select `UserName`,`Tel`,`UserId`,`Id`,`Address`,`Remark`,`ActType`,`Ip`,`GoodsName`,`GoodsId`,`RealName` from lotteryuserinfo where `Id`=#{id}
	</select>

</mapper>
