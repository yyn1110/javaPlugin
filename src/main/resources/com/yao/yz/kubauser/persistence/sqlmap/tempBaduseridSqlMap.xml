<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.TempBaduseridDao">

	<!--属性-->
	<parameterMap id="tempBaduseridParameterMap" type="com.yao.yz.kubauser.persistence.model.TempBaduserid">
		<parameter property="userid"/>
		<parameter property="会员级别"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="tempBaduseridResultMap" type="com.yao.yz.kubauser.persistence.model.TempBaduserid">
		<result property="userid" column="userid"/>
		<result property="会员级别" column="会员级别"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="tempBaduseridParameterMap" keyProperty="userid" useGeneratedKeys="true">
		insert into temp_baduserid(`userid`,`会员级别`) values(?,?)
	</insert>

	<!--更新-->
	<update id="update" parameterMap="tempBaduseridParameterMap">
		update temp_baduserid set
		<trim suffixOverrides=",">
			<if test="会员级别 != null">
				`会员级别`=#{会员级别},
			</if>
		</trim>
		where `userid`=#{userid}
	</update>

	<!--删除-->
	<delete id="delete" parameterType="java.lang.Integer">
		delete from temp_baduserid where `userid`=#{userid}
	</delete>

	<!--查询-->
	<select id="getTempBaduseridByKey" resultMap="tempBaduseridResultMap" parameterType="java.lang.Integer">
		select `userid`,`会员级别` from temp_baduserid where `userid`=#{userid}
	</select>

</mapper>
