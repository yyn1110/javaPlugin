<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.TempCouponUserDao">

	<!--属性-->
	<parameterMap id="tempCouponUserParameterMap" type="com.yao.yz.kubauser.persistence.model.TempCouponUser">
		<parameter property="username"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="tempCouponUserResultMap" type="com.yao.yz.kubauser.persistence.model.TempCouponUser">
		<result property="username" column="username"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="tempCouponUserParameterMap">
		insert into temp_coupon_user(`username`) values(?)
		<!--没有主键-->
	</insert>

	<!--更新-->
	<update id="update" parameterMap="tempCouponUserParameterMap">
		update temp_coupon_user set
		<trim suffixOverrides=",">
			<if test="username != null">
				`username`=#{username},
			</if>
		</trim>
		<!--没有主键-->
	</update>

	<!--删除-->
	<!--没有主键-->

	<!--查询-->
	<!--没有主键-->

</mapper>
