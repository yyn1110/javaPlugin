<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.TempTelephoneDao">

	<!--属性-->
	<parameterMap id="tempTelephoneParameterMap" type="com.yao.yz.kubauser.persistence.model.TempTelephone">
		<parameter property="telephone"/>
		<parameter property="code"/>
		<parameter property="activecode"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="tempTelephoneResultMap" type="com.yao.yz.kubauser.persistence.model.TempTelephone">
		<result property="telephone" column="telephone"/>
		<result property="code" column="code"/>
		<result property="activecode" column="activecode"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="tempTelephoneParameterMap">
		insert into temp_telephone(`telephone`,`code`,`activecode`) values(?,?,?)
		<!--没有主键-->
	</insert>

	<!--更新-->
	<update id="update" parameterMap="tempTelephoneParameterMap">
		update temp_telephone set
		<trim suffixOverrides=",">
			<if test="telephone != null">
				`telephone`=#{telephone},
			</if>
			<if test="code != null">
				`code`=#{code},
			</if>
			<if test="activecode != null">
				`activecode`=#{activecode},
			</if>
		</trim>
		<!--没有主键-->
	</update>

	<!--删除-->
	<!--没有主键-->

	<!--查询-->
	<!--没有主键-->

</mapper>
