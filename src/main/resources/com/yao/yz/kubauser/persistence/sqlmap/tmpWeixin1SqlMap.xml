		<parameter property="email"/>
		<parameter property="loginEmail"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="tmpWeixin1ResultMap" type="com.yao.yz.kubauser.persistence.model.TmpWeixin1">
		<result property="userId" column="user_id"/>
		<result property="id" column="Id"/>
		<result property="email" column="Email"/>
		<result property="loginEmail" column="LoginEmail"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="tmpWeixin1ParameterMap">
		insert into tmp_weixin1(`user_id`,`Id`,`Email`,`LoginEmail`) values(?,?,?,?)
		<!--没有主键-->
	</insert>

	<!--更新-->
	<update id="update" parameterMap="tmpWeixin1ParameterMap">
		update tmp_weixin1 set
		<trim suffixOverrides=",">
			<if test="userId != null">
				`user_id`=#{userId},
			</if>
			<if test="id != null">
				`Id`=#{id},
			</if>
			<if test="email != null">
				`Email`=#{email},
			</if>
			<if test="loginEmail != null">
				`LoginEmail`=#{loginEmail},
			</if>
		</trim>
		<!--没有主键-->
	</update>

	<!--删除-->
	<!--没有主键-->

	<!--查询-->
	<!--没有主键-->

</mapper>
