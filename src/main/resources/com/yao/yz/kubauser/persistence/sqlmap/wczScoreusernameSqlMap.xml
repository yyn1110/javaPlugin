<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.WczScoreusernameDao">

	<!--属性-->
	<parameterMap id="wczScoreusernameParameterMap" type="com.yao.yz.kubauser.persistence.model.WczScoreusername">
		<parameter property="score"/>
		<parameter property="username"/>
		<parameter property="datetime"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="wczScoreusernameResultMap" type="com.yao.yz.kubauser.persistence.model.WczScoreusername">
		<result property="score" column="score"/>
		<result property="username" column="username"/>
		<result property="datetime" column="datetime"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="wczScoreusernameParameterMap">
		insert into wcz_scoreusername(`score`,`username`,`datetime`) values(?,?,?)
		<!--没有主键-->
	</insert>

	<!--更新-->
	<update id="update" parameterMap="wczScoreusernameParameterMap">
		update wcz_scoreusername set
		<trim suffixOverrides=",">
			<if test="score != null">
				`score`=#{score},
			</if>
			<if test="username != null">
				`username`=#{username},
			</if>
			<if test="datetime != null">
				`datetime`=#{datetime},
			</if>
		</trim>
		<!--没有主键-->
	</update>

	<!--删除-->
	<!--没有主键-->

	<!--查询-->
	<!--没有主键-->

</mapper>
