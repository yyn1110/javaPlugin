<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.WczUserlogincountDao">

	<!--属性-->
	<parameterMap id="wczUserlogincountParameterMap" type="com.yao.yz.kubauser.persistence.model.WczUserlogincount">
		<parameter property="userid"/>
		<parameter property="c"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="wczUserlogincountResultMap" type="com.yao.yz.kubauser.persistence.model.WczUserlogincount">
		<result property="userid" column="userid"/>
		<result property="c" column="c"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="wczUserlogincountParameterMap">
		insert into wcz_userlogincount(`userid`,`c`) values(?,?)
		<!--没有主键-->
	</insert>

	<!--更新-->
	<update id="update" parameterMap="wczUserlogincountParameterMap">
		update wcz_userlogincount set
		<trim suffixOverrides=",">
			<if test="userid != null">
				`userid`=#{userid},
			</if>
			<if test="c != null">
				`c`=#{c},
			</if>
		</trim>
		<!--没有主键-->
	</update>

	<!--删除-->
	<!--没有主键-->

	<!--查询-->
	<!--没有主键-->

</mapper>
