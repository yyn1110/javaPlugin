<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.YdyRecipeLogDao">

	<!--属性-->
	<parameterMap id="ydyRecipeLogParameterMap" type="com.yao.yz.kubauser.persistence.model.YdyRecipeLog">
		<parameter property="id"/>
		<parameter property="voucherId"/>
		<parameter property="recipeId"/>
		<parameter property="recipeUrl"/>
		<parameter property="createtime"/>
		<parameter property="callBack"/>
		<parameter property="orderNo"/>
		<parameter property="isDel"/>
		<parameter property="operaterId"/>
		<parameter property="operaterName"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="ydyRecipeLogResultMap" type="com.yao.yz.kubauser.persistence.model.YdyRecipeLog">
		<result property="id" column="id"/>
		<result property="voucherId" column="voucher_id"/>
		<result property="recipeId" column="recipe_id"/>
		<result property="recipeUrl" column="recipe_url"/>
		<result property="createtime" column="createtime"/>
		<result property="callBack" column="call_back"/>
		<result property="orderNo" column="order_no"/>
		<result property="isDel" column="is_del"/>
		<result property="operaterId" column="operaterId"/>
		<result property="operaterName" column="operaterName"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="ydyRecipeLogParameterMap" keyProperty="id" useGeneratedKeys="true">
		insert into ydy_recipe_log(`id`,`voucher_id`,`recipe_id`,`recipe_url`,`createtime`,`call_back`,`order_no`,`is_del`,`operaterId`,`operaterName`) values(?,?,?,?,?,?,?,?,?,?)
	</insert>

	<!--更新-->
	<update id="update" parameterMap="ydyRecipeLogParameterMap">
		update ydy_recipe_log set
		<trim suffixOverrides=",">
			<if test="voucherId != null">
				`voucher_id`=#{voucherId},
			</if>
			<if test="recipeId != null">
				`recipe_id`=#{recipeId},
			</if>
			<if test="recipeUrl != null">
				`recipe_url`=#{recipeUrl},
			</if>
			<if test="createtime != null">
				`createtime`=#{createtime},
			</if>
			<if test="callBack != null">
				`call_back`=#{callBack},
			</if>
			<if test="orderNo != null">
				`order_no`=#{orderNo},
			</if>
			<if test="isDel != null">
				`is_del`=#{isDel},
			</if>
			<if test="operaterId != null">
				`operaterId`=#{operaterId},
			</if>
			<if test="operaterName != null">
				`operaterName`=#{operaterName},
			</if>
		</trim>
		where `id`=#{id}
	</update>

	<!--删除-->
	<delete id="delete" parameterType="java.lang.Integer">
		delete from ydy_recipe_log where `id`=#{id}
	</delete>

	<!--查询-->
	<select id="getYdyRecipeLogByKey" resultMap="ydyRecipeLogResultMap" parameterType="java.lang.Integer">
		select `id`,`voucher_id`,`recipe_id`,`recipe_url`,`createtime`,`call_back`,`order_no`,`is_del`,`operaterId`,`operaterName` from ydy_recipe_log where `id`=#{id}
	</select>

</mapper>
