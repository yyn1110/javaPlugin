<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yao.yz.kubauser.persistence.dao.ZsjVenderUserDao">

	<!--属性-->
	<parameterMap id="zsjVenderUserParameterMap" type="com.yao.yz.kubauser.persistence.model.ZsjVenderUser">
		<parameter property="venderId"/>
		<parameter property="userName"/>
		<parameter property="mobile"/>
	</parameterMap>

	<!--数据库字段与对象属性映射-->
	<resultMap id="zsjVenderUserResultMap" type="com.yao.yz.kubauser.persistence.model.ZsjVenderUser">
		<result property="venderId" column="vender_id"/>
		<result property="userName" column="user_name"/>
		<result property="mobile" column="mobile"/>
	</resultMap>

	<!--新增-->
	<insert id="insert" parameterMap="zsjVenderUserParameterMap">
		insert into zsj_vender_user(`vender_id`,`user_name`,`mobile`) values(?,?,?)
		<!--没有主键-->
	</insert>

	<!--更新-->
	<update id="update" parameterMap="zsjVenderUserParameterMap">
		update zsj_vender_user set
		<trim suffixOverrides=",">
			<if test="venderId != null">
				`vender_id`=#{venderId},
			</if>
			<if test="userName != null">
				`user_name`=#{userName},
			</if>
			<if test="mobile != null">
				`mobile`=#{mobile},
			</if>
		</trim>
		<!--没有主键-->
	</update>

	<!--删除-->
	<!--没有主键-->

	<!--查询-->
	<!--没有主键-->

</mapper>
